{% extends "target.default.j2" %}

{% block variables %}

ccflags = $ccflags $
    -nostdlib $
    -nodefaultlibs $
    -fno-builtin $
    -mno-sse $
    -fno-omit-frame-pointer $
    -g $
    -ggdb $
    -isystem${srcroot}/src/include $
    -Wno-float-equal $
    -Wno-memset-transposed-args

libs = $libs $
    -L${builddir} $
    -lc

{% endblock %}

